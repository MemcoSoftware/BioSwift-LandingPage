{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:scss": "sass src/scss/main.scss public/css/main.css --style compressed",
    "watch:scss": "sass --watch src/scss/main.scss:public/css/main.css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.14.2",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.15.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "sass": "^1.83.4",
    "typescript": "^4.8.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/src/styles"
    }
  ]
}
